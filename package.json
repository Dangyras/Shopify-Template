{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile_scss": "sass --no-source-map dev/sass/main.scss:assets/custom.main_styles.css dev/sass/product.scss:assets/custom.product_styles.css dev/sass/collection.scss:assets/custom.collection_styles.css dev/sass/homepage.scss:assets/custom.homepage_styles.css",
    "compile_scss_compressed": "sass --style compressed --no-source-map dev/sass/main.scss:assets/custom.main_styles.min.css dev/sass/product.scss:assets/custom.product_styles.min.css dev/sass/collection.scss:assets/custom.collection_styles.min.css dev/sass/homepage.scss:assets/custom.homepage_styles.min.css",
    "compile_javascript": "cp dev/js/main.js assets/custom.main_scripts.js & terser dev/js/main.js -o assets/custom.main_scripts.min.js --compress --mangle",
    "compile": "sudo npm run compile_scss & sudo npm run compile_scss_compressed & sudo npm run compile_javascript",
    "compile_watch": "sass --watch --style compressed --no-source-map dev/sass/main.scss:assets/custom.main_styles.min.css dev/sass/product.scss:assets/custom.product_styles.min.css dev/sass/collection.scss:assets/custom.collection_styles.min.css dev/sass/homepage.scss:assets/custom.homepage_styles.min.css",
    "push_env1": "sudo shopify theme push -e env1",
    "deploy_env1": "sudo npm run compile & sudo npm run push_env1"
  },
  "author": "",
  "license": "ISC"
}
