{% liquid
  assign ss = section.settings
  assign breakpoint_large = settings.breakpoint_large | default: 1200
  assign breakpoint_medium = settings.breakpoint_medium | default: 992
  assign breakpoint_small = settings.breakpoint_small | default: 768
  assign breakpoint_tiny = settings.breakpoint_tiny | default: 556
%}
{% liquid
  assign ss = section.settings
  assign breakpoint_large = settings.breakpoint_large | default: 1200
  assign breakpoint_medium = settings.breakpoint_medium | default: 992
  assign breakpoint_small = settings.breakpoint_small | default: 768
  assign breakpoint_tiny = settings.breakpoint_tiny | default: 556
%}

{%- case type -%}

{%- when 'simple_image' -%}
  <div class="image--wrapper">
    <img
      {% assign sizes = "360.00, 556.00, 768.00, 992.00, 1200.00, 1540.00, 300:1650" | split: ", " %}
      {% if ss.width == "" %}
      {% endif %}
      {% assign image_object = ss.image %}
      src="{{ image_object | image_url: width: sizes[0] }}"
      loading="lazy"
      alt="{{ image_object.alt | default: ss.title | escape }}"
      width="{{ image_object.width }}"
      height="{{ image_object.width | divided_by: image_object.aspect_ratio | round }}"
      srcset="{% render 'custom-srcset', image: image_object, image_settings: sizes.last %}"
      sizes="
        (min-width: {{ breakpoint_largeX }}px) {{ sizes[5] }}px,
        (min-width: {{ breakpoint_large }}px) {{ sizes[4] | divided_by: breakpoint_large | times: 100 | round }}vw,
        (min-width: {{ breakpoint_medium }}px) {{ sizes[3] | divided_by: breakpoint_medium | times: 100 | round }}vw,
        (min-width: {{ breakpoint_small }}px) {{ sizes[2] | divided_by: breakpoint_small | times: 100 | round }}vw,
        (min-width: {{ breakpoint_tiny }}px) {{ sizes[1] | divided_by: breakpoint_tiny | times: 100 | round }}vw,
        {{ sizes[0] | divided_by: breakpoint_base | times: 100 | round }}vw
      "
    >
  </div>

{%- when 'form_template' -%}
  <div class="section--content">
    {% if ss.image != blank %}
      <div class="section-aside">
        <h2 class="heading heading--h3">{{ ss.title }}</h2>

        <ul class="list list--none">
          <li>{{ ss.subtitle  }}</li>
          {% for block in section.blocks %}
            <li>
              <div class="icon--wrapper" style="background-image: url({{ block.settings.image |  image_url: width: 50 }});"></div>
              <div class="text--wrapper">{{ block.settings.text }}</div>
            </li>
          {% endfor %}
        </ul>

        <div class="image--wrapper">
          <img
            src="{{ ss.image |  image_url: width: 20 }}"
            loading="lazy"
            alt="{{ ss.title }}"
            width="{{ ss.image.width }}"
            height="{{ ss.image.width | divided_by: ss.image.aspect_ratio }}"
            srcset="{% render "custom-srcset", image: ss.image, start: 360, end: 650, gap: 40 %}"
            sizes="
              (min-width: {{ breakpoint_large | default: "1200" }}px) {{ 580 | append: "px" }},
              (min-width: {{ breakpoint_medium | default: "992" }}px) {{ 432.00 | divided_by: 992 | times: 100 | round | append: "vw" }},
              (min-width: {{ breakpoint_small | default: "768" }}px) {{ 328.00 | divided_by: 768 | times: 100 | round | append: "vw" }},
              (min-width: {{ breakpoint_tiny | default: "556" }}px)  {{ 540.00 | divided_by: 556 | times: 100 | round | append: "vw" }},
              {{ 360.00 | divided_by: 360 | times: 100 | round | append: "vw" }}
            "
          >
        </div>
      </div>
    {% endif %}
    <div class="form--wrapper">
      {%- liquid
        assign contact_form_class = 'isolate'
        if settings.animations_reveal_on_scroll
          assign contact_form_class = 'isolate scroll-trigger animate--slide-in'
        endif
      -%}
      {%- form 'contact', id: 'ContactForm', class: contact_form_class -%}
        {%- if form.posted_successfully? -%}
          <h2 class="form-status form-status-list form__message" tabindex="-1" autofocus>
            {{- 'icon-success.svg' | inline_asset_content -}}
            {{ 'templates.contact.form.post_success' | t }}
          </h2>
        {%- elsif form.errors -%}
          <div class="form__message">
            <h2 class="form-status caption-large text-body" role="alert" tabindex="-1" autofocus>
              {{- 'icon-error.svg' | inline_asset_content -}}
              {{ 'templates.contact.form.error_heading' | t }}
            </h2>
          </div>
          <ul class="form-status-list caption-large" role="list">
            <li>
              <a href="#ContactForm-email" class="link">
                {{ form.errors.translated_fields.email | capitalize }}
                {{ form.errors.messages.email }}
              </a>
            </li>
          </ul>
        {%- endif -%}

        <div class="field--group">
          <div class="field">
            <label class="field__label" for="ContactForm-name">{{ 'templates.contact.form.name' | t }}</label>
            <input
              class="field__input"
              autocomplete="name"
              type="text"
              id="ContactForm-name"
              name="contact[{{ 'templates.contact.form.name' | t }}]"
              value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}"
              placeholder="{{ 'custom.contact.first_name_placeholder' | t }}"
            >
          </div>

          <div class="field">
            <label class="field__label" for="ContactForm-name">PavardÄ—</label>
            <input
              class="field__input"
              autocomplete="name"
              type="text"
              id="ContactForm-name"
              name="contact[last_{{ 'templates.contact.form.name' | t }}]"
              value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}"
              placeholder="{{ 'custom.contact.last_name_placeholder' | t }}"
            >
          </div>
        </div>

        
        <div class="field--group">
          <div class="field field--with-error">
            <label class="field__label" for="ContactForm-email">{{- 'templates.contact.form.email' | t }} <span aria-hidden="true">*</span></label>
            <input
              autocomplete="email"
              type="email"
              id="ContactForm-email"
              class="field__input"
              name="contact[email]"
              spellcheck="false"
              autocapitalize="off"
              value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
              aria-required="true"
              {% if form.errors contains 'email' %}
                aria-invalid="true"
                aria-describedby="ContactForm-email-error"
              {% endif %}
              placeholder="{{ 'custom.contact.mail_placeholder' | t }}"
            >
            {%- if form.errors contains 'email' -%}
              <small class="contact__field-error" id="ContactForm-email-error">
                <span class="visually-hidden">{{ 'accessibility.error' | t }}</span>
                <span class="form__message">
                  <span class="svg-wrapper">
                    {{- 'icon-error.svg' | inline_asset_content -}}
                  </span>
                  {{- form.errors.translated_fields.email | capitalize }}
                  {{ form.errors.messages.email -}}
                </span>
              </small>
            {%- endif -%}
          </div>
          <div class="field">
            <label class="field__label" for="ContactForm-phone">{{ 'templates.contact.form.phone' | t }}</label>
            <input
              type="tel"
              id="ContactForm-phone"
              class="field__input"
              autocomplete="tel"
              name="contact[{{ 'templates.contact.form.phone' | t }}]"
              pattern="[0-9\-]*"
              value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}"
              placeholder="{{ 'custom.contact.phone_placeholder' | t }}"
            >
          </div>
        </div>

        <div class="field">
          <label class="form__label field__label" for="ContactForm-body">
            {{- 'templates.contact.form.comment' | t -}}
          </label>
          <textarea
            rows="10"
            id="ContactForm-body"
            class="text-area field__input"
            name="contact[{{ 'templates.contact.form.comment' | t }}]"
            placeholder="{{ 'custom.contact.message_placeholder' | t }}"
          >
            {{- form.body -}}
          </textarea>
        </div>
        
        <div class="contact__button">
          <button type="submit" class="button button--primary">
            {{ 'templates.contact.form.send' | t }}
          </button>
        </div>
      {%- endform -%}
    </div>
  </div>

{%- when 'simple_banner' -%}
  <div class="section--content">
    <div class="grid--wrapper grid--sm-1">
      <div class="grid--item  {% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
        {% if ss.title != blank or ss.text != blank %}
          <div class="content-header ">
            {% if ss.title != blank %}
              {% if ss.h1 %}
                <h1 class="heading heading--{% if ss.small %}h2{% else %}h1{% endif %} {% if ss.decoration %}style--decoration{% endif %}">{{ ss.title }}</h1>
              {% else %}
                <h2 class="heading heading--{% if ss.small %}h2{% else %}h1{% endif %} {% if ss.decoration %}style--decoration{% endif %}">{{ ss.title }}</h2>
              {% endif %}
            {% endif %}
      
            {% if ss.text != blank %}
              <div class="text--wrapper">{{ ss.text | replace: '<ul', '<ul class="list list--image"' }}</div>
            {% endif %}

            {% if ss.button != blank %}
              <div class="button--wrapper">{{ ss.button | replace: 'href', 'class="button--primary" href' }}</div>
            {% endif %}
          </div>
        {% endif %}
        
      </div>

      <div class="grid--item element-image">
        {% if ss.image_desktop != blank %}
          <div data-style="--mobile-bg: url({{ ss.image_mobile | image_url: width: 2000 }}); --mob-ratio: {{ ss.aspect_m }}; --desktop-bg: url({{ ss.image_desktop | image_url: width: 2000 }}); --desktop-ratio: {{ ss.aspect_d }};" 
          class="image--wrapper style--bg bg-lazy">

          </div>
        {% else %}
          <div class="image--wrapper">
            <img
              src="{{ ss.image |  image_url: width: 20 }}"
              loading="lazy"
              alt="{{ ss.title }}"
              width="{{ ss.image.width }}"
              height="{{ ss.image.width | divided_by: ss.image.aspect_ratio }}"
              srcset="{% render "custom-srcset", image: ss.image, start: 360, end: 650, gap: 40 %}"
              sizes="
                (min-width: {{ breakpoint_large | default: "1200" }}px) {{ 580 | append: "px" }},
                (min-width: {{ breakpoint_medium | default: "992" }}px) {{ 432.00 | divided_by: 992 | times: 100 | round | append: "vw" }},
                (min-width: {{ breakpoint_small | default: "768" }}px) {{ 328.00 | divided_by: 768 | times: 100 | round | append: "vw" }},
                (min-width: {{ breakpoint_tiny | default: "556" }}px)  {{ 540.00 | divided_by: 556 | times: 100 | round | append: "vw" }},
                {{ 360.00 | divided_by: 360 | times: 100 | round | append: "vw" }}
              "
            >
          </div>
        {% endif %}
      </div>
    </div>
  </div>

{%- when 'carousel' -%}
  <div class="section--content">
    {% if ss.title != blank or ss.text_1 != blank %}
      <div class="content-header ">
          
        {% if ss.title != blank %}
          {% if ss.h1 %}
            <h1 class="heading heading--h1 {% if ss.decoration %}style--decoration{% endif %}">{{ ss.title }}</h1>
          {% else %}
            <h2 class="heading heading--h1 {% if ss.decoration %}style--decoration{% endif %}">{{ ss.title }}</h2>
          {% endif %}
        {% endif %}
  
        {% if ss.text_1 != blank %}
          <div class="text--wrapper">{{ ss.text_1 }}</div>
        {% endif %}
      </div>
    {% endif %}
    <slider-element data-snippet="slider_element" style="--desktop-size:  {{ ss.slides }}; --tablet-size:  {{ ss.slides_tablet }}; --mobile-size:  {{ ss.slides_mobile }};" class="slider-element" 
    data-settings='{"slidesPerView": {{ ss.slides_mobile }}, "slidesPerGroup": 1, "autoplay": {% if ss.autoplay_delay != null and ss.autoplay_delay > 0 %}{ "delay" : {{ ss.autoplay_delay | times: 1000 }} }{% else %} false{% endif %}, "centeredSlides": {{ ss.center }}, "loop": {% if ss.loop %}true{% else %}false{% endif %}, "spaceBetween": {{ ss.gap_mobile }},
          "breakpoints": {
            "{{ breakpoint_small }}": {"slidesPerView": {{ ss.slides_tablet }}, "slidesPerGroup": {{ ss.group_desktop }},"centeredSlides": {{ ss.center }}, "spaceBetween": {{ ss.gap }}},
            "{{ breakpoint_large }}": {"slidesPerView": {{ ss.slides }}, "slidesPerGroup": {{ ss.group_desktop }}, "centeredSlides": {{ ss.center }}, "spaceBetween": {{ ss.gap }}}
          }}'>
      <div class="swiper">
        <div class="swiper-wrapper">
          {% for block in section.blocks %}
            <div class="swiper-slide">  
              <div class="slide--inner select--none">
                {% assign width = breakpoint_large | divided_by: ss.slides %}
                
                {% assign slides_desktop = ss.slides | plus: 0 %}
                {% assign slides_tablet = ss.slides_tablet | plus: 0 %}
                {% assign slides_mobile = ss.slides_mobile | plus: 0 %}
                
                {% assign image_width_desktop = 1200.00 | divided_by: slides_desktop %}
                {% assign image_width_tablet = 768.00 | divided_by: slides_tablet %}
                {% assign image_width_mobile = 360.00 | divided_by: slides_mobile %}
                {% assign image_width_desktop2 = 1200 | divided_by: slides_desktop %}

                {% if ss.width == "default2" and slides_desktop == 1 %}
                  {% assign image_width_desktop = 1900.00 %}
                  {% assign image_width_desktop2 = 1900 %}
                {% endif %}
                <div>
                  {% if block.settings.image_mobile != blank %}
                    <picture data-test="{{ image_width_desktop }}">
                      <source
                        media="(max-width: {{ breakpoint_small }}px)"
                        
                        width="{{ block.settings.image_mobile.width }}"
                        height="{{ block.settings.image_mobile.width | divided_by: block.settings.image_mobile.aspect_ratio }}"
                        srcset="{% render "custom-srcset", image: block.settings.image_mobile, start: 150, end: image_width_desktop2, gap: 30 %}"
                        sizes="
                          (min-width: {{ breakpoint_large | default: "1200" }}px) {{ image_width_desktop | append: "px" }},
                          (min-width: {{ breakpoint_small | default: "768" }}px) {{ image_width_tablet | divided_by: 768 | times: 100 | round | append: "vw" }},
                          {{ image_width_mobile | divided_by: 360 | times: 100 | round | append: "vw" }}
                        "
                      />
                      <source
                        media="(min-width: {{ breakpoint_small }}px)"
                        width="{{ block.settings.image.width }}"
                        height="{{ block.settings.image.width | divided_by: block.settings.image.aspect_ratio }}"
                        srcset="{% render "custom-srcset", image: block.settings.image, start: 700, end: image_width_desktop2, gap: 130 %}"
                        sizes="
                          (min-width: {{ breakpoint_large | default: "1200" }}px) {{ image_width_desktop | append: "px" }},
                          (min-width: {{ breakpoint_small | default: "768" }}px) {{ image_width_tablet | divided_by: 768 | times: 100 | round | append: "vw" }},
                          {{ image_width_mobile | divided_by: 360 | times: 100 | round | append: "vw" }}
                        "
                      />
                      <img class="image" src="{{ block.settings.image_mobile | img_url: "385x" }}" alt="{{ block.settings.image_mobile.alt }}"
                        loading="eager"
                        fetchpriority="high"
                        decoding="async"
                      />
                    </picture>
                  {% else %}

                  
                    {% if block.settings.url != blank %}
                      <a href="{{ block.settings.url }}" class="image--wrapper">
                        <img
                          data-test="{{ image_width_tablet }}"
                          {% if forloop.first %}data-height="set"{% endif %}
                          src="{{ block.settings.image |  image_url: width: 20 }}"
                          loading="lazy"
                          alt="{{ block.settings.title }}"
                          width="{{ block.settings.image.width }}"
                          height="{{ block.settings.image.width | divided_by: block.settings.image.aspect_ratio }}"
                          srcset="{% render "custom-srcset", image: block.settings.image, start: 150, end: image_width_desktop2, gap: 30 %}"
                          sizes="
                            (min-width: {{ breakpoint_large | default: "1200" }}px) {{ image_width_desktop | append: "px" }},
                            (min-width: {{ breakpoint_small | default: "768" }}px) {{ image_width_tablet | divided_by: 768 | times: 100 | round | append: "vw" }},
                            {{ image_width_mobile | divided_by: 360 | times: 100 | round | append: "vw" }}
                          "
                        >
                      </a>
                    {% else %}
                    <div class="image--wrapper">
                      <img
                        data-test="{{ image_width_tablet }}"
                        {% if forloop.first %}data-height="set"{% endif %}
                        src="{{ block.settings.image |  image_url: width: 20 }}"
                        loading="lazy"
                        alt="{{ block.settings.title }}"
                        width="{{ block.settings.image.width }}"
                        height="{{ block.settings.image.width | divided_by: block.settings.image.aspect_ratio }}"
                        srcset="{% render "custom-srcset", image: block.settings.image, start: 150, end: image_width_desktop2, gap: 30 %}"
                        sizes="
                          (min-width: {{ breakpoint_large | default: "1200" }}px) {{ image_width_desktop | append: "px" }},
                          (min-width: {{ breakpoint_small | default: "768" }}px) {{ image_width_tablet | divided_by: 768 | times: 100 | round | append: "vw" }},
                          {{ image_width_mobile | divided_by: 360 | times: 100 | round | append: "vw" }}
                        "
                      >
                    </div>
                    {% endif %}

                  {% endif %}

                  {% if block.settings.button != blank %}
                    <div class="button--wrapper">{{ block.settings.button }}</div>
                  {% endif %}
                </div>
              </div>
              </div>
          {% endfor %}
        </div>
      </div>
      {% if ss.navigation_style == "1" %}
        <div class="swiper-button-prev show--smUp"></div>
        <div class="swiper-button-next show--smUp"></div>
          
        <div class="swiper-pagination"></div>
      {% elsif ss.navigation_style == "2" %}
        <div class="container container--{{ ss.width }}">
          <div class="navigation--wrapper">
            <div class="counter--wrapper">
              <div class="swiper-counter" data-current="01" data-total="0{{ section.blocks.size }}">01 / <span>0{{ section.blocks.size }}</span></div>
            </div>

            <div class="pagination--wrapper">
              <div class="swiper-pagination style--progressbar"></div>
            </div>
            <div class="arrows--wrapper">
              <div class="swiper-button-prev show--smUp0">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20"><path fill="currentColor" d="M6.287 11.038h9.74v-1.6h-9.74l4.48-4.48-1.14-1.12-6.4 6.4 6.4 6.4 1.14-1.12-4.48-4.48Z"/></svg>
              </div>
              <div class="swiper-button-next show--smUp0">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20"><path fill="currentColor" d="M13.713 11.038h-9.74v-1.6h9.74l-4.48-4.48 1.14-1.12 6.4 6.4-6.4 6.4-1.14-1.12 4.48-4.48Z"/></svg>
              </div>
            </div>
          </div>
        </div>
      {% else %}
        <div class="swiper-button-prev show--smUp"></div>
        <div class="swiper-button-next show--smUp"></div>
          
        <div class="swiper-pagination"></div>
      {% endif %}
    </slider-element>
  </div>

{%- when 'collection_categories' -%}
  <div class="section--content">
    {% assign categories = collection.metafields.custom.categories.value %}
    {% if categories == blank %}
      <style>#section__{{ section.id }} { display: none !important;}</style>
    {% endif %}
    {% for category in categories %}
      <div class="slider--wrapper">
        <h2 class="heading heading--h1">{{ category.title }}</h2>
        {% liquid 
          assign category_items = category.collections.value
          assign slides_desktop = category.slides_desktop | default: 4
          assign slides_mobile = category.slides_mobile | default: 1.2
          assign slides_tablet = slides_desktop | minus: slides_mobile
          assign is_slider = "true"
          if category.slider == false
            assign is_slider = "false"
          endif
        %}
        
        {% if is_slider == "true" %}
        <slider-element 
          data-snippet="slider_element" 
          style="--desktop-size:  {{ slides_desktop }}; --tablet-size:  {{ slides_tablet }}; --mobile-size:  {{ slides_mobile }};" 
          class="slider-element style--even" 
          data-settings='{
            "slidesPerView": {{ slides_mobile }}, 
            "centeredSlides": false,
            "autoplay": false, 
            "loop": true, 
            "spaceBetween": 16,
            "breakpoints": {
              "{{ breakpoint_small }}": {"slidesPerView": {{ slides_tablet }}, "centeredSlides": false, "spaceBetween": 16},
              "{{ breakpoint_large }}": {"slidesPerView": {{ slides_desktop }}, "centeredSlides": false, "spaceBetween": 16}
            }
          }'
        >
          
          <div class="swiper-button-prev show--smUp"></div>

          <div class="swiper">
            <div class="swiper-wrapper">
              {% for category_item in category_items %}
                <div class="swiper-slide">  
                  <div class="slide--inner select--none">
                    {% render "custom-snippets", type: "category-card", content: category_item, slides_desktop: slides_desktop, slides_mobile: slides_mobile, slides_tablet: slides_tablet, first: forloop.first %}
                  </div>
                </div>
              {% endfor %}
            </div>
          </div>
                
          <div class="swiper-button-next show--smUp"></div>
          <div class="swiper-pagination hide--lgUp"></div>
        </slider-element>
        {% else %}
          <div class="grid--wrapper">
            
              {% for category_item in category_items %}
                <div class="grid--item">  
                  <div class="slide--inner select--none">
                    {% render "custom-snippets", type: "category-card", content: category_item, slides_desktop: slides_desktop, slides_mobile: slides_mobile, slides_tablet: slides_tablet, first: forloop.first %}
                  </div>
                </div>
              {% endfor %}
          </div>
        {% endif %}
      </div>
    {% endfor %}
  </div>

{%- when 'liquid_container' -%}
  <div class="section--content">
    <div class="liquid--wrapper">
      {{ ss.liquid_code }}
    </div>
  </div>

