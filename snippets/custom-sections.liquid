            sizes="
              (min-width: {{ breakpoint_large | default: "1200" }}px) {{ 529 | append: "px" }},
              (min-width: {{ breakpoint_medium | default: "992" }}px) {{ 432.00 | divided_by: 992 | times: 100 | round | append: "vw" }},
              (min-width: {{ breakpoint_small | default: "768" }}px) {{ 328.00 | divided_by: 768 | times: 100 | round | append: "vw" }},
              (min-width: {{ breakpoint_tiny | default: "556" }}px)  {{ 540.00 | divided_by: 556 | times: 100 | round | append: "vw" }},
              {{ 360.00 | divided_by: 360 | times: 100 | round | append: "vw" }}
            "
          >
        </div>
      </div>
    </div>
  </div>


{%- when 'icons' -%}
  <div class="section--content">
    <div class="section--content  text--center">
      {% if ss.heading != blank or ss.text_1 != blank %}
        <div class="content-header ">
          {% if ss.heading != blank %}
            {% if ss.h1 %}
              <h1 class="heading heading--h1 {% if ss.decoration %}style--decoration{% endif %}"><span>{{ ss.heading }}</span></h1>
            {% else %}
