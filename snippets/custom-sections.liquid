              {% if ss.h1 %}
                <h1 class="heading heading--h1 {% if ss.decoration %}style--decoration{% endif %}"><span>{{ ss.heading }}</span></h1>
              {% else %}
                <h2 class="heading heading--h1 {% if ss.decoration %}style--decoration{% endif %}"><span>{{ ss.heading }}</span></h2>
              {% endif %}
            {% endif %}

            {% if ss.text_1 != blank %}
              <div class="text--wrapper {% if ss.center %}text--center{% endif %}">{{ ss.text_1 }}</div>
            {% endif %}
          </div>
        {% endif %}

        {% if ss.text != blank %}
          <div class="text--wrapper style--colourAlt style--{{ ss.size }} {% if ss.center %}text--center{% endif %}">
            {{ ss.text | replace: "section__title", "heading heading--h2 text--center style--decoration" }}
          </div>
        {% endif %}
        
        {% if template contains "collection" %}
          <style>
            #section__{{ section.id }} .faq {
              text-align: center;
            }
          </style>
          <script>
            const convertFAQ = () => {
              document.querySelectorAll('#section__{{ section.id }} .faq__wrapper .accordion-item').forEach((item, index) => {
                  const question = item.querySelector('.accordion-header')?.childNodes[0]?.textContent.trim();
                  const answer = item.querySelector('.accordion-content .inner-content')?.textContent.trim();
                  
                  if (question && answer) {
                      const dropdown = document.createElement('dropdown-element');
                      const uniqueId = `custom_dropdowns_gAh4MR${index + 1}_header`;
                      
                      dropdown.innerHTML = `
                          <button type="button" class="button--reset dropdown-header" aria-controls="${uniqueId}" data-dropwdown="trigger" aria-expanded="false">
                              <span>${question}</span>
                              <svg class="arrow" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 12 7">
                                  <path stroke="#001C70" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 1 6 6 1 1"></path>
                              </svg>
                          </button>
                          <div id="${uniqueId}" data-dropwdown="body">
                              <div class="dropdown--inner" data-title="Header ${index + 1}" data-index="${index + 1}">
                                  <p>${answer}</p>
                              </div>
                          </div>
                      `;
                      
                      item.replaceWith(dropdown);
                  }
              });
            };

            document.addEventListener("DOMContentLoaded", convertFAQ);
          </script>
        {% endif %}
        {%  if ss.heading == blank and ss.text_1 == blank and ss.text == blank %}
          <style>
            #section__{{ section.id }} {
              display: none;
            }
          </style>
        {% endif %}
      {% endif %}
  </div>

